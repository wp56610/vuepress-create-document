{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name demo ./src/index.js",
    "build:test": "vue-cli-service build --target lib --test test./src/index.js",
    "lint": "vue-cli-service lint",
    "document": "npm run build && vuese gen && gulp && vuese serve --open"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "through2": "^4.0.2",
    "vue": "^2.6.11",
    "vuepress": "^1.7.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuese": "^1.4.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
